@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityRole>


@{
    ViewData["Title"] = "ListOfRoles";
}

<h1>ListOfRoles</h1>


@* <a asp-action="Create">Create</a> *@
<table class="table table-striped table-bordered">
    <thead>
        <tr>
            <td>Id</td>
            <td>Role</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var role in Model)
        {
            <tr>
                <th>@role.Id</th>
            </tr>

             <tr>
                <th>@role.Name</th>

            </tr>
        }
    </tbody>
</table>
